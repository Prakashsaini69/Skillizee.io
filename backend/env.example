# SkilliZee Lambda Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Razorpay Configuration
RAZORPAY_KEY_ID=rzp_live_your_key_id_here
RAZORPAY_KEY_SECRET=your_razorpay_secret_here
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret_here

# Graphy LMS Configuration
GRAPHY_MERCHANT_ID=your_graphy_merchant_id
GRAPHY_API_TOKEN=your_graphy_api_token

# Course Configuration
COURSE_ID=your_course_id_here

# AWS Configuration (optional - will use AWS CLI default)
AWS_REGION=us-east-1
AWS_PROFILE=default

# Environment
NODE_ENV=production
STAGE=dev
